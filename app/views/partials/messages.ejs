
<div class="row ">
    <% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error) { %>
        <div class="col-lg-4 mx-auto pt-5">
            <div class="bs-component ">
                <div class="alert alert-dismissible alert-danger">
                    <button class="close" type="button" data-dismiss="alert">×</button><%= error.msg %>
                </div>
            </div>
        </div>
        <% }); %> <% } %> <% if(success_msg != ''){ %>
        <div class="col-lg-4 mx-auto pt-5">
            <div class="bs-component">
                <div class="alert alert-dismissible alert-success">
                    <button class="close" type="button" data-dismiss="alert">×</button> <%= success_msg %>
                </div>
            </div>
        </div>
        <% } %> <% if(error_msg != ''){ %>
        <div class="col-lg-4 mx-auto pt-5">
            <div class="bs-component">
                <div class="alert alert-dismissible alert-danger">
                    <button class="close" type="button" data-dismiss="alert">×</button><%= error_msg %>
                </div>
            </div>
        </div>
        <% } %> <% if(error != ''){ %>
        <div class="col-lg-4 mx-auto pt-5">
            <div class="bs-component">
                <div class="alert alert-dismissible alert-danger">
                    <button class="close" type="button" data-dismiss="alert">×</button> <%= error %>
                </div>
            </div>
        </div>
    <% } %>
    
</div>
